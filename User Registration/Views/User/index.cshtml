@model IEnumerable<User_Registration.Entities.User>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>View</title>
</head>
<body>
    <table>
        <tr style ="border:solid 1px black">
            <th style="border:solid 1px black">First Name</th>
            <th style="border:solid 1px black">Last Name</th>
            <th style="border:solid 1px black">Age</th>
            <th style="border:solid 1px black">ImagePath</th>
        </tr>
        @foreach (var item in Model)
       {
            <tr >
                <th style="border:solid 1px black">@item.Name</th>
                <th style="border:solid 1px black">@item.Surname</th>
                <th style="border:solid 1px black">@item.Age</th>
                <th style="border:solid 1px black">@item.ImagePath</th>
           </tr>
       }
       </table>

    <input type="button" value="Go to registration" onclick="location.href='@Url.Action("Add", "User")'" />
</body>
</html>